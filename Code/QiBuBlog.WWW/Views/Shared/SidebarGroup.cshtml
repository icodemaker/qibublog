@model List<QiBuBlog.Entity.DTO.ArticleGroup>
@if (Model != null)
{
    <section>
        <h2 class="titlebar">归档</h2>
        <ol class="sidebar-articlelist">
            @foreach (var group in Model)
            {
                <li class="clearfix">
                    <div class="sidebar-articlerank-viewcount"></div>
                    <div class="sidebar-articlerank-title"><a href="javascript:;" target="_blank" title="@group.GroupName">@Convert.ToDateTime(group.GroupName).ToString("yyyy年MM月")&nbsp;(@group.Amount)</a></div>
                </li>
            }
        </ol>
    </section>
}